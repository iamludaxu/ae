<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gift.witch.android.ae">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".base.AeApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".butterKnife.ButterKnifeActivity"/>
        <activity android:name=".logger.LoggerActivity"/>
        <activity android:name=".okhttp.OkHttpActivity"/>
        <activity android:name=".retrofit2.Retrofit2Activity"/>
        <activity android:name=".rxjava.RxJavaActivity"/>
        <activity android:name=".display.DisplayActivity"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>